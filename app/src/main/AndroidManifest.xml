<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.lance.widget.demo"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".CommonTopBarDemoActivity">
        </activity>
        <activity android:name=".CustomTopBarDemoActivity">
        </activity>
        <activity android:name=".CommonOptionViewDemoActivity">
        </activity>
        <activity android:name=".MultiImageViewDemoActivity">
        </activity>
        <activity android:name=".SwipeLayoutDemoActivity">
        </activity>
        <activity android:name=".FullDelDemoActivity">
        </activity>
        <activity android:name=".LinearLayoutDelDemoActivity">
        </activity>
        <activity android:name=".ListViewDelDemoActivity">
        </activity>
        <activity android:name=".DialogDemoActivity">
        </activity>
        <activity android:name=".ImageBannerActivity">
        </activity>
        <activity android:name=".EasyPickerViewDemoActivity">
        </activity>
        <activity android:name=".CommonPopupWindowDemoActivity">
        </activity>
        <activity android:name=".FlowLayoutDemoActivity">
        </activity>
    </application>

</manifest>